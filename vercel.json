{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/predict-digit",
            "destination": "/api/predict.py"
        },
        {
            "source": "/static/(.*)",
            "destination": "/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/public/$1"
        },
        {
            "source": "/",
            "destination": "/public/index.html"
        }
    ]
}